---
const trackingId = 'G-SMBGDNDVZ1';
---

<head>
  <script
    async
    src={`https://www.googletagmanager.com/gtag/js?id=${trackingId}`}></script>
  <script>
    // @ts-ignore
    window.dataLayer = window.dataLayer || [];
    function gtag() {
      // @ts-ignore
      dataLayer.push(arguments);
    }
    // @ts-ignore
    gtag('js', new Date());
    // @ts-ignore
    gtag('config', '${trackingId}');
  </script>
</head>
